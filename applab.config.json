{
  "build": [
    {
      "buildDir": "dist",
      "buildFile": "index.es.js",
      "buildTypes": false,
      "bundle": true,
      "codeSplitting": true,
      "excludeLibs": [
        "react",
        "react-dom",
        "react-password-strength-bar",
        "@stripe/react-stripe-js"
      ],
      "define": {},
      "format": "esm",
      "inputScripts": ["src/index.tsx"],
      "minify": false,
      "outDir": "dist",
      "platform": "browser",
      "sourceMap": true,
      "target": "es2020",
      "treeShaking": true
    },
    {
      "buildDir": "dist",
      "buildFile": "index.cjs.js",
      "buildTypes": false,
      "bundle": true,
      "codeSplitting": false,
      "excludeLibs": [
        "react",
        "react-dom",
        "react-password-strength-bar",
        "@stripe/react-stripe-js"
      ],
      "define": {},
      "format": "cjs",
      "inputScripts": ["src/index.tsx"],
      "minify": false,
      "platform": "browser",
      "sourceMap": true,
      "target": "es2015",
      "treeShaking": true
    }
  ]
}
